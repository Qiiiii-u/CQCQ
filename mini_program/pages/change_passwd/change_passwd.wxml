<!--pages/chang_passwd/chang_passwd.wxml-->
<form>
	<view class="cu-form-group">
		<input password='true' placeholder='请输入新密码' bindinput='passwdInput' value='{{newpwd}}'></input>
	</view>
	<view class="cu-form-group">
		<input password='true' placeholder='请确认一遍密码' bindinput='repasswdInput' value='{{newpwd2}}'></input>
	</view>
	<view class="flex padding flex-direction">
		<button class="cu-btn bg-grey lg" style="width:700rpx;background-color:#7EC4F8" bindtap="onLaunch">提交</button>
	</view>
</form>