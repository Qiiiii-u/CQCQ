<!--pages/phone/phone.wxml-->
<form>
	<view class="cu-form-group margin-top">
		<input placeholder="请输入手机号" bindinput='phoneInput'></input>
	</view>
	<view class="cu-form-group">
		<input placeholder="请输入验证码" bindinput='numInput'></input>
		<button class="cu-btn bg-white" style="width:200rpx;color:#7EC4F8" bindtap='onClick' disabled='{{disabled}}'>{{codename}}</button>
	</view>
	<view class="padding flex flex-direction">
		<button class="cu-btn bg-grey lg" style="width:700rpx;background-color:#7EC4F8" bindtap='onInput'>下一步</button>
	</view>
</form>